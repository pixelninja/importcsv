    <types>
        <type>Workflow</type>
        <type>Other</type>
    </types>
	<authors>
	</authors>
	<releases>
        <release version="0.4" date="2017-07-19" min="2.7.x"/>
        <release version="0.3" date="2011-12-15" min="2.3"/>
		<release version="0.2.3" date="2011-08-31" min="2.2.x"/>
	</releases>
</extension>
